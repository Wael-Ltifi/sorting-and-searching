PROCEDURE ins_sort(VAR arr : ARRAY_OF INTEGER)
VAR
    i,j,cur : INTEGER;
BEGIN
    FOR i FROM 1 TO arr.length-1 DO
        cur := arr [i];
        j := i-1;
        WHILE (j >= 0 AND arr[j] > cur) DO
            arr[j+1] := arr[j];
            j := j-1;
        END_WHILE
        arr[j+1] = cur;
    END_FOR
END